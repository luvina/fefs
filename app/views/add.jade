ion-view(title='Add vehicle')
  ion-content(has-header='true', has-tabs='true')
    .card
      .item.item-text-wrap
        h2 Add Vehicle Here


       .item.item-input.item-stacked-label
        span.input-label Name:
        input(type='text' placeholder="write name here" ng-model='vehicle.name')
       .item.item-input.item-stacked-label
           
       label Type <br/>
          select(ng-model='vehicle.type', style='width:100%')
            option sedan
            option toyota
            option XLI

     .row
       .col
              label Upload image

              input.button.button-block(type='file', ng-model="vehicle.image", base-sixty-four-input)

     .row
       .col

              button.button.button-block(ng-click='addVehicle()' id='save') Save








